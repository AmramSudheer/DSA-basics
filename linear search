array = [3,4,5,6,7,8,9,2,1]
x = 6
n = len(array)
def Linearsearch(array,n,x):
    for i in range(0,n):
        if array[i] == x:
            return i 
    return -1

result = Linearsearch(array,n,x)
if result == -1:
    print("element is not found")
else:
    print(f"element is found: index [{result}]")
